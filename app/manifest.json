{
  "name": "__MSG_appName__",
  "version": "0.0.8",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "https://localhost:7001/posui/*",
    "https://localhost:7002/posui/*",
    "https://localhost:7003/posui/*",
    "https://cwposdev.cricketwireless.com/posui/*",
    "https://cwpostest.cricketwireless.com/posui/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://localhost:7001/posui/*",
        "https://localhost:7002/posui/*",
        "https://localhost:7003/posui/*",
        "https://cwposdev.cricketwireless.com/posui/*",
        "https://cwpostest.cricketwireless.com/posui/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "TestNgSuiteBuilderFromJenkinsFailedTestsPage",
    "default_popup": "popup.html"
  }
}
